msgstr ""
"  -b,  --background                siirry taustalle käynnistyksen jälkeen.\n"

#: src/main.c:438
msgid "  -e,  --execute=COMMAND   execute a `.wgetrc'-style command.\n"
msgstr ""
"  -e,  --execute=KOMENTO           suorita ”.wgetrc”-tyylinen komento.\n"

#: src/main.c:442
msgid "Logging and input file:\n"
